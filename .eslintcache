e(0);\n\n    const goLeft = () => {\n        x === 0 ? \n            setX((sliderArr.length - 1) * -100)\n            :\n            setX(x + 100)\n        };\n\n    const goRight= () => {\n        x === (sliderArr.length - 1) * -100 ?\n            setX(0)\n            :\n            setX(x - 100)\n        };  \n    \n    useEffect(() => {\n        const id = setTimeout(() => goRight(), 3000);\n        return () => clearTimeout(id);\n    }, [x]);\n\n    return(\n        <div className={sliderStyle.slider}>\n            { sliderArr.map((item, idx) => {\n                return(\n                    <div key={idx} className={sliderStyle.slide} style={{ transform: `translateX(${x}%)`}}>\n                        {item}\n                    </div>\n                );\n            })}\n            <button id={sliderStyle.goLeft} onClick={() => goLeft()}><FontAwesomeIcon icon={ fas, faChevronCircleLeft } className={sliderStyle.i} size=\"3x\" /></button>\n            <button id={sliderStyle.goRight} onClick={() => goRight()}><FontAwesomeIcon icon={ fas, faChevronCircleRight} className={sliderStyle.i} size=\"3x\" /></button>\n        </div>\n    );\n}\n\nexport default Slider; ",["140","141"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/login/login.js",["142"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/form-button/form-button.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/categoryItem/categoryItem.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/pages/homepage/homepage.js",["143"],"\nimport React, { Component } from 'react';\nimport homepageStyles from './homepage.module.scss';\nimport Directory from '../../components/directory/directory';\nimport Slider from '../../components/slider/slider';\n\nimport { withRouter } from \"react-router-dom\";\n\nclass Homepage extends Component {\n    constructor(props) {\n    