n  }\n\n  render(){\n\n    return (\n      <>\n        <Header />\n        <div className=\"wholePage\">\n          <Switch>\n            <Route exact path=\"/\" component={Homepage} />\n            <Route exact path=\"/shop\" component={Shoppage} />\n            <Route exact path=\"/shop/:category\" component={Categorypage} />\n            <Route exact path=\"/login\" render={()=> this.props.currentUser ? (<Redirect to=\"/\"  />) : (<Loginpage />)} />\n            <Route exact path=\"/checkout\" component={CheckoutPage}></Route>\n          </Switch>\n        </div>   \n        <Footer />\n      </>\n    )\n  }\n}\n\nconst mapStateToProps = createStructuredSelector({\n  currentUser: selectCurrentUser\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n})\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);",["117","118"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/pages/loginpage/loginpage.js",["119"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/header/header.js",[],["120","121"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/redux/user/user-reducer.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/stripe-button/stripe-button.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/checkout-product/checkout-product.js",["122","123","124","125"],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/redux/cart/cart-utils.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/components/slider-image/slider-image.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/reportWebVitals.js",[],"/Users/ghl/Desktop/collectorsReduxReactApp/react-redux-ecommerce-store/src/pages/categorypage/categorypage.js",[],["126","127"],"/Users/ghl/Desktop/col