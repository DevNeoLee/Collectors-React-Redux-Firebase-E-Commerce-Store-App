v className={loginStyle.id}>\n                <h1>Login</h1>\n    \n                <form onSubmit={this.handleSubmit}>\n                    <FormInput\n                        name='email'\n                        type='email'\n                        handleChange={this.handleChange}\n                        value={this.state.email}\n                        label='email'\n                        required\n                    />\n                    <FormInput\n                        name='password'\n                        type='password'\n                        value={this.state.password}\n                        handleChange={this.handleChange}\n                        label='password'\n                        required\n                    />\n                    <FormButton type='submit'> Sign in </FormButton>\n                    <FormButton onClick={ signInWithGoogle }> Sign in with Google </FormButton>\n                </form>\n            </div>\n        )\n    }\n}\n\nexport default Login\n\n",["163","164"],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/form-button/form-button.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/form-input/form-input.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/firebase/utils.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/pages/shoppage/shop.data.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/user/user-reducer.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/cart-header/cart-header.js",[],["165","166"],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/cart-dropdown/cart-dropdown.js",[],["167","168"],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/root-reducer.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/cart/cart-types.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/cart/cart-actions.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/user/user-types.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/cart/cart-reducer.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/user/user-actions.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/cart/cart-utils.js",["169","170"],"export const addProductToCart = (cartProducts, clickedProduct) => {\n    const alreadyAdded = cartProducts.find(product => \n        product.id === clickedProduct.id\n    );\n\n    if (alreadyAdded) {\n        return (cartProducts.map(product =>\n                (product.id === clickedProduct.id) ? \n                    {...product, quantity: product.quantity + 1} \n                    : product\n        ));        \n    } else {\n        return [...cartProducts, {...clickedProduct, quantity: 1}];\n    }\n};\n\nexport const increaseQuantity = (cartProducts, payload) => {\n         return cartProducts.map((product) => {\n                    product.id === payload.id ? {...product, quantity: product.quantity + 1} : product\n    });\n};\n","/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/cart-item-header/cart-item-header.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/cart/cart-selectors.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/user/user-selectors.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/pages/checkoutpage/checkoutpage.js",["171"],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/components/checkout-product/checkout-product.js",[],"/Users/ghl/Desktop/react-redux-ecommerce-store/src/redux/store.js",["172"],{"ruleId":"173","replacedBy":"174"},{"ruleId":"175","replacedBy":"176"},{"ruleId":"173","replacedBy":"177"},{"ruleId":"175","replacedBy":"178"},{"ruleId":"173","replacedBy":"179"},{"ruleId":"175","replacedBy":"180"},{"ruleId":"173","replacedBy":"181"},{"ruleId":"175","replacedBy":"182"},{"ruleId":"173","replacedBy":"183"},{"ruleId":"175","replacedBy":"184"},{"ruleId":"185","severity":1,"message":"186","line":2,"column":10,"nodeType":"18